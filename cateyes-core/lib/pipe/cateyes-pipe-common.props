<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <ItemDefinitionGroup>
    <ClCompile>
      <PreprocessorDefinitions>G_LOG_DOMAIN=&quot;Cateyes&quot;;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
  </ItemDefinitionGroup>

  <ItemGroup>
    <CustomBuild Include="pipe.vala">
      <Message>Compiling vala code</Message>
      <Command>"$(ValaCompiler)" pipe.vala -D WINDOWS --ccode --directory=$(IntDir) --library=cateyes-pipe --header=$(IntDir)cateyes-pipe.h $(ValaFlags) --pkg=gio-2.0 --pkg=gee-0.8 || exit 1
echo &gt; "$(IntDir)valacode.stamp"
</Command>
      <AdditionalInputs>$(ValaCompiler);%(AdditionalInputs)</AdditionalInputs>
      <Outputs>$(IntDir)valacode.stamp;%(Outputs)</Outputs>
    </CustomBuild>
  </ItemGroup>

  <ItemGroup>
    <ClInclude Include="$(IntDir)cateyes-pipe.h" />
    <ClInclude Include="pipe-glue.h" />
    <ClInclude Include="pipe-sddl.h" />
  </ItemGroup>

  <ItemGroup>
    <ClCompile Include="$(IntDir)pipe.c" />
    <ClCompile Include="pipe-windows.c" />
    <ClCompile Include="pipe-sddl.c" />
  </ItemGroup>

</Project>
